    "mongoose": "^5.10.11",
    "node-mailer": "^0.1.1",
    "sanitize-filename": "^1.6.3",
    "sanitize-html": "^1.22.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.37",
    "@types/nodemailer": "^6.4.1",
    "@types/sanitize-html": "^2.3.1",
    "dependency-cruiser": "^9.26.0",
    "madge": "^4.0.2",
    "mocha": "^7.1.2",
    "nyc": "^15.1.0",
    "typescript": "^4.2.4"
